<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Banca.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Banca.Maui"
    xmlns:pages="clr-namespace:Banca.Maui.Pages"    
    Title="Banca.Maui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Ahorros"
        ContentTemplate="{DataTemplate pages:AhorrosPage}"
        Route="AhorrosPage" />

    <ShellContent
        Title="Periodos"
        ContentTemplate="{DataTemplate pages:PeriodosPage}"
        Route="PeriodosPage" />

</Shell>
